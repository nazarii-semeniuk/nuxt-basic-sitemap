{
  "name": "nuxt-basic-sitemap",
  "version": "0.3.1",
  "description": "Basic sitemap module for Nuxt",
  "repository": "https://github.com/nazarii-semeniuk/nuxt-basic-sitemap",
  "license": "MIT",
  "type": "module",
  "keywords": [
    "nuxt sitemap",
    "nuxt",
    "sitemap",
    "nuxt simple sitemap",
    "nuxt basic sitemap"
  ],
  "author": "Nazarii Semeniuk <qnazar.semenyuk@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist",
    "virtual.d.ts"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish --access public && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run --silent false",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.4",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.1",
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/eslint-config": "^1.4.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.4",
    "@nuxt/test-utils": "^3.19.1",
    "@types/node": "latest",
    "changelogen": "^0.6.1",
    "eslint": "^9.27.0",
    "nuxt": "^3.17.4",
    "prettier": "^3.5.3",
    "typescript": "~5.8.3",
    "vitest": "^3.1.4",
    "vue-tsc": "^2.2.10"
  }
}
